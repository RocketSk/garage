<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
            prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.kursach.controller.userController.SignUpController">
    <children>
        <VBox alignment="TOP_CENTER" layoutX="180.0" layoutY="132.0" prefHeight="400.0" prefWidth="600.0"
              AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
              AnchorPane.topAnchor="0.0">
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Логин">
                    <VBox.margin>
                        <Insets top="20.0"/>
                    </VBox.margin>
                </Text>
                <TextField fx:id="loginField">
                    <VBox.margin>
                        <Insets left="100.0" right="100.0" top="10.0"/>
                    </VBox.margin>
                </TextField>
                <Text layoutX="10.0" layoutY="23.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Придумайте пароль">
                    <VBox.margin>
                        <Insets top="20.0"/>
                    </VBox.margin>
                </Text>
                <PasswordField fx:id="passwordField">
                    <VBox.margin>
                        <Insets left="100.0" right="100.0" top="10.0"/>
                    </VBox.margin>
                </PasswordField>
                <Text layoutX="289.0" layoutY="117.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Повторите пароль">
                    <VBox.margin>
                        <Insets top="20.0"/>
                    </VBox.margin>
                </Text>
                <PasswordField fx:id="repPasswordField" layoutX="110.0" layoutY="132.0">
                    <VBox.margin>
                        <Insets left="100.0" right="100.0" top="10.0"/>
                    </VBox.margin>
                </PasswordField>
                <Button mnemonicParsing="false" onAction="#signUp" text="Регистрация">
                    <VBox.margin>
                        <Insets top="30.0"/>
                    </VBox.margin>
                </Button>
                <Button mnemonicParsing="false" onAction="#cancel" text="Отмена">
                    <VBox.margin>
                        <Insets top="20.0"/>
                    </VBox.margin>
                </Button>
            </children>
        </VBox>
    </children>
</AnchorPane>
